class st:
    def __init__(self,num,name,grade):
        self.num = num
        self.name = name
        self.grade = grade
    def __repr__(self):
        return repr((self.num, self.name, self.grade))
stList = [
    st(1,'A',80),
    st(2,'B',85),
    st(3,'C',45),
    st(4,'D',87),
    st(5,'E',63),
    st(6,'F',84),
    st(7,'G',83),
    st(8,'H',90),
    st(9,'J',100),
    st(10,'K',56)
    ]

print(sorted(stList, key=lambda st: st.grade))


